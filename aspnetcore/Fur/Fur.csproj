    <Authors>百小僧/MonkSoul</Authors>
    <Company>Baiqian</Company>
    <Copyright>Baiqian</Copyright>
    <Description>小僧不才，略懂皮毛（Fur）。</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SwaggerGen\Assets\MiniProfilerIndex.html" />
