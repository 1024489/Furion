		<SatelliteResourceLanguages>en-US</SatelliteResourceLanguages>
	</PropertyGroup>

	<ItemGroup>
		<Content Remove="wwwroot\images\logo.png" />
	</ItemGroup>

	<ItemGroup>
		<None Include="wwwroot\images\logo.png" />
	</ItemGroup>

	<ItemGroup>
