			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\FurionRazorApi.Web.Core\FurionRazorApi.Web.Core.csproj" />
	</ItemGroup>

</Project>
